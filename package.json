{
  "name": "@orangejellyfish/serverless-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "deploy": "export NODE_ENV=dev && npm run deploy:env",
    "deploy:env": "sls deploy --stage $NODE_ENV",
    "deploy:production": "export NODE_ENV=production && npm run deploy:env",
    "lint": "eslint ./src",
    "precommit": "lint-staged",
    "start": "serverless offline start",
    "test": "jest --watch src"
  },
  "dependencies": {
    "aws-sdk": "^2.368.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "serverless": "^1.34.1",
    "serverless-iam-roles-per-function": "^1.0.4",
    "serverless-offline": "^3.31.3",
    "serverless-plugin-stage-variables": "^1.8.2",
    "serverless-webpack": "^5.2.0",
    "webpack": "^4.26.1",
    "webpack-node-externals": "^1.7.2"
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint -- --fix",
      "git add"
    ]
  },
  "jest": {
    "collectCoverage": true
  }
}
